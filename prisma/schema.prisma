generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShipmentOrder {
  id                String    @id @default(cuid())
  createdAt         DateTime  @default(now())
  lineNickname      String?
  recipientName     String
  address           String
  phone             String
  idNumber          String?
  calculationResult Json      
  
  @@map("shipment_orders")
}